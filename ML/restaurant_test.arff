% test arff file for R&N restaurant decision tree example
%
@relation restaurant

@attribute AlternateNearby {Yes, No}
@attribute HasBar  {Yes, No}
@attribute IsFridayOrSaturday  {Yes, No}
@attribute Hungry  {Yes, No}
@attribute HowCrowded  {None, Some, Full}
@attribute Price  {$, $$, $$$}
@attribute Raining  {Yes, No}
@attribute Reservations {Yes, No}
@attribute Type  {French, Thai, Burger, Italian}
@attribute WaitingTime {0-10, 10-30, 30-60, >60}
